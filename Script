local function checkHook()
    local funcs = {gg.alert, os.clock, string.format, debug.getinfo}
    for _, func in ipairs(funcs) do
        if tostring(func):match("function: builtin") then
            return true
        end
    end
    return false
end

if checkHook() then
    while true do end 
end

