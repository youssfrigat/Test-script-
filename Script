    gg.setVisible(false)
    _G.gg["searchNumber"]("h 00 00 00 00 3f 00 00 80 3e", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
    gg.setVisible(false)
    _G.gg["getResults"](400)
