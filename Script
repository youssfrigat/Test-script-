    _G.gg["editAll"]("h FF FF FF FF 08 00 00 00 00 00 60 40 CD CC 8C 3F 8F C2 F5 3C CD CC CC 3D 06 00 00 00 00 00 00 00 00 00 00 00 00 00 F0 41", gg.TYPE_BYTE)
    gg.setVisible(false)
    _G.gg["toast"]("AIMFOV AWM DISACTIVED âŒ")
    
    _G.gg["clearResults"]()
    _G.gg["clearResults"]()
